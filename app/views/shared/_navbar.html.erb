<ul id="main-nav-menu" class="navbar-nav d-flex flex-md-row flex">
  <%= render_list_items do |li|
    li << link_to(t("menu.topics"), main_app.root_path, class: "nav-link block p-4 border-b-4 border-white hover:border-red-400")

    Homeland.navbar_plugins.each do |plugin|
      li << link_to(plugin.display_name, plugin.root_path, class: "nav-link block p-4")
    end
  end %>
  <%= raw Setting.navbar_html %>
</ul>
